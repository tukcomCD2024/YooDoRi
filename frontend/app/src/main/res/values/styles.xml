    <style name="Widget.App.Button" parent="Widget.AppCompat.Button">
        <item name="background">@drawable/rectangle_corner_10</item>
        <item name="backgroundTint">@color/yellow</item>
        <item name="android:textAppearance">@style/TextAppearance.App.20sp.Bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">12dp</item>
    </style>

    <style name="Widget.App.TextInputLayout.corner10" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxCornerRadiusBottomEnd">10dp</item>
        <item name="boxCornerRadiusBottomStart">10dp</item>
        <item name="boxCornerRadiusTopEnd">10dp</item>
        <item name="boxCornerRadiusTopStart">10dp</item>
        <item name="errorEnabled">true</item>
        <item name="endIconMode">clear_text</item>
        <item name="errorIconDrawable">@null</item>
        <item name="errorTextAppearance">@style/TextAppearance.App.16sp.SemiBold</item>
        <item name="errorTextColor">@color/orange</item>
    </style>

    <style name="Widget.MyApp.BottomNavigationView.Material2" parent="Widget.MaterialComponents.BottomNavigationView"></style>

    <style name="Widget.MyApp.FloatingActionButton.Material2" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="android:elevation">0dp</item>
    </style>

    <style name="textStroke">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">#B28D39</item>
        <item name="android:padding">1dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
